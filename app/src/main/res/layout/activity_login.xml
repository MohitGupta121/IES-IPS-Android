<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.LoginActivity"
    android:background="@drawable/background">

    <ImageView
        android:id="@+id/login_backgrounnd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_35sdp"
        android:layout_marginTop="@dimen/_150sdp"
        android:layout_marginEnd="@dimen/_35sdp"
        android:layout_marginBottom="@dimen/_276sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.728"
        app:srcCompat="@drawable/rectangle_2" />

    <ImageView
        android:id="@+id/logoies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_102sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_30sdp"
        app:layout_constraintBottom_toTopOf="@+id/login_backgrounnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logoies" />

    <ImageView
        android:id="@+id/roundwhite"
        android:layout_width="@dimen/_60sdp"
        android:layout_height="@dimen/_70sdp"
        android:layout_marginStart="@dimen/_245sdp"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/login_backgrounnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/login_backgrounnd"
        app:layout_constraintVertical_bias="0.538"
        app:srcCompat="@drawable/white" />

    <ImageView
        android:id="@+id/login_button_image_submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/roundwhite"
        app:layout_constraintEnd_toEndOf="@+id/roundwhite"
        app:layout_constraintHorizontal_bias="0.961"
        app:layout_constraintStart_toStartOf="@+id/login_backgrounnd"
        app:layout_constraintTop_toTopOf="@+id/roundwhite"
        app:layout_constraintVertical_bias="0.502"
        app:srcCompat="@drawable/blue" />


    <ImageView
        android:id="@+id/Login"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        app:layout_constraintBottom_toBottomOf="@+id/roundwhite"
        app:layout_constraintEnd_toEndOf="@+id/login_button_image_submit"
        app:layout_constraintHorizontal_bias="0.729"
        app:layout_constraintStart_toStartOf="@+id/roundwhite"
        app:layout_constraintTop_toTopOf="@+id/roundwhite"
        app:layout_constraintVertical_bias="0.486"
        app:srcCompat="@drawable/ic_forward_arrow" />

    <EditText
        android:id="@+id/edit_text_computer_code_login"
        android:layout_width="@dimen/_225sdp"
        android:layout_height="@dimen/_45sdp"
        android:background="@drawable/input"
        android:drawablePadding="@dimen/_5sdp"
        android:drawableStart="@drawable/ic_computer"
        android:ems="10"
        android:fontFamily="@font/varela_round"
        android:hint="@string/computer_code"
        android:inputType="none|number"
        android:textColor="#000000"
        android:textColorHint="#707070"
        app:layout_constraintBottom_toTopOf="@+id/edit_text_password_login"
        app:layout_constraintEnd_toEndOf="@+id/login_backgrounnd"
        app:layout_constraintStart_toStartOf="@+id/login_backgrounnd"
        app:layout_constraintTop_toTopOf="@+id/login_backgrounnd"
        app:layout_constraintVertical_bias="0.447"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <EditText
        android:id="@+id/edit_text_password_login"
        android:layout_width="@dimen/_225sdp"
        android:layout_height="@dimen/_45sdp"
        android:background="@drawable/input"
        android:drawableStart="@drawable/ic_lock"
        android:ems="10"
        android:fontFamily="@font/varela_round"
        android:hint="@string/password"
        android:drawablePadding="5dp"
        android:inputType="textPassword"
        android:minHeight="48dp"
        android:textColor="#000000"
        android:textColorHint="#707070"
        app:layout_constraintBottom_toBottomOf="@+id/login_backgrounnd"
        app:layout_constraintEnd_toEndOf="@+id/login_backgrounnd"
        app:layout_constraintStart_toStartOf="@+id/login_backgrounnd"
        app:layout_constraintTop_toTopOf="@+id/login_backgrounnd"
        app:layout_constraintVertical_bias="0.819"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/computercode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/varela_round"
        android:text="@string/computer_code"
        android:textColor="#22367F"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintBottom_toTopOf="@+id/edit_text_computer_code_login"
        app:layout_constraintEnd_toStartOf="@+id/login_button_image_submit"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="@+id/login_backgrounnd"
        app:layout_constraintTop_toTopOf="@+id/login_backgrounnd"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/password"
        android:textColor="#22367F"
        android:textSize="@dimen/_15ssp"
        app:layout_constraintBottom_toBottomOf="@+id/login_backgrounnd"
        app:layout_constraintEnd_toStartOf="@+id/roundwhite"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="@+id/login_backgrounnd"
        app:layout_constraintTop_toTopOf="@+id/computercode"
        app:layout_constraintVertical_bias="0.562"
        tools:text="Password" />


</androidx.constraintlayout.widget.ConstraintLayout>